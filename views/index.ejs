<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel='stylesheet' href='/css/style.css' />
    <title>PDF Catalog</title>
</head>

<body>
    <form action="fileupload" method="post" enctype="multipart/form-data">
        <input id="name" type="file" name="filetoupload"><br>
        <input type="submit">
    </form>
    <form action="/path" method="post">
        <label>Title:</label>
        <input type="text" value="" name="fileTitleInput">
        <input type="submit">
    </form>

    <% pdfLog.forEach(function(dat) { %>
    <div class="row">
        <div class="column">
            <div class="card">
                <img src="<%= dat.imagePath %>" style="width:100%">
                <div class="container">
                    <h2><%= dat.fileName%></h2>
                    <p><button class="button" onclick="window.location.href ='<%= dat.filePath %>';">Open</button></p>
                </div>
            </div>
        </div>
        <% }); %>
</body>

</html>